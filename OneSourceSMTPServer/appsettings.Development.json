{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information",
      "Hangfire": "Information"
    }
  },
  "SMTPReceiver": {
    "ReceivedEmailsDirectory": "C:\\SMTPReceiver\\ReceivedEmails"
  },
  "DataRetentionPolicy": {
    "TimeForPersistingDataInMonths": 1,
    "DeleteOldLogsAndEmailsCronExpression": "0 0 1 * *"
  },
  "ConnectionStrings": {
    "OneSourceContextConnection": "Server=localhost\\MSSQLSERVER01;Database=OneSourceMasterLocal;Trusted_Connection=True;MultipleActiveResultSets=true;TrustServerCertificate=True;Command Timeout=300;"
  }
}
