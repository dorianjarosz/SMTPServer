{
  "Logging": {
    "LogLevel": {
      "Default": "Error",
      "Microsoft.Hosting.Lifetime": "Error",
      "Hangfire": "Error"
    },
    "EventSource": {
      "LogLevel": {
        "Default": "Error",
        "Microsoft.Hosting.Lifetime": "Error",
        "Hangfire": "Error"
      }
    },
    "EventLog": {
      "LogLevel": {
        "Default": "Error",
        "Microsoft.Hosting.Lifetime": "Error",
        "Hangfire": "Error"
      }
    }
  },
  "SMTPReceiver": {
    "ReceivedEmailsDirectory": "C:\\SMTPReceiver\\ReceivedEmails",
    "HandleEmailIntervalCronExpression": "*/1 * * * *"
  },
  "DataRetentionPolicy": {
    "TimeForPersistingDataInMonths": 1,
    "DeleteOldLogsAndEmailsCronExpression": "0 0 1 * *"
  },
  "ConnectionStrings": {
    "OneSourceContextConnection": "Server=localhost;Database=s81;Trusted_Connection=True;MultipleActiveResultSets=true;TrustServerCertificate=True;Command Timeout=300;"
  }
}
